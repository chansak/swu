<!DOCTYPE html>
<html lang="en" ng-app="app" ng-controller="RootController">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="Keywords" lang="en" content="{{$parent.seo.keywordEn}}">
    <meta name="Keywords" lang="th" content="{{$parent.seo.keywordTh}}">
    <meta name="Description" content="{{$parent.seo.description}}">
    @*<link rel="shortcut icon" href="favicon.ico" type="image/x-icon"/>*@
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <title>{{$parent.seo.title}}</title>

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
            <script src="~/Content/js/html5shiv.min.js"></script>
            <script src="~/Content/js/respond.min.js"></script>
        <![endif]-->
    @Styles.Render("~/theme/css", "~/Content/css")
</head>
<body ng-app="app" layout="column" ng-cloak>
    <div layout="column" layout-fill>
        @RenderBody()
    </div>
    <link href="~/Content/css/summernote.css" rel="stylesheet" />
    <script src="~/Content/js/summernote.min.js"></script>
    @Scripts.Render("~/bundles/ng-lib")
    @Scripts.Render("~/bundles/ng-app", "~/bundles/utilities-lib")
    <script src="~/Content/js/angular-summernote.min.js"></script>
    @RenderSection("scripts", required: false)
    <!-- Google Map Javascript Codes -->
    <script src="http://maps.google.com/maps/api/js?key=AIzaSyDBQQS6mDPwPe8pEtHpL4wwn9iGxyQtpTI"></script>
    <script src="~/Content/js/googlemaps.js"></script>
</body>
</html>
